# Gate M5 mainnet-only defaults.

# Locked genesis identities
LOCKED_MAINNET_GENESIS_HASH=692a347dab52762df864509bc9b0972408d9dc778ef0851190b18bb1555e1be5
LOCKED_TESTNET_GENESIS_HASH=1b9c1803328d95518a0fd921ce8fd1d5f93c9a88ca02c0b1440248effc763159

# Where dist artifacts are copied on each host
ARTIFACT_DIR=/opt/kexa/artifacts

# Installation locations
INSTALL_DIR=/opt/kexa
BIN_DIR=/opt/kexa/bin
ETC_DIR=/etc/kexa
GENESIS_MAINNET_PATH=/etc/kexa/genesis-mainnet.json

# Mainnet node identity
NODE_ROLE=seed1
NETWORK=mainnet

# Safe mainnet defaults (do not reuse testnet ports)
MAINNET_P2P_PORT=9040
MAINNET_RPC_PORT=18040

# Seed-specific settings
SEED1_PUBLIC_P2P_IP=203.0.113.10
SEED2_PUBLIC_P2P_IP=203.0.113.11
SEED1_P2P_PORT=9040
SEED2_P2P_PORT=9040
SEED1_RPC_PORT=18040
SEED2_RPC_PORT=18040

# Local listen addresses
RPC_BIND_HOST=127.0.0.1
P2P_BIND_HOST=0.0.0.0

# Data/log/proof locations
DATA_DIR_MAINNET=/var/lib/kexa/mainnet
LOG_DIR=/var/log/kexa
PROOF_ROOT=/var/lib/kexa/proof

# Optional public RPC for diagnostics only (keep false by default)
ALLOW_PUBLIC_RPC=false
PUBLIC_RPC_BIND_HOST=0.0.0.0

# Joiner parameters
JOINER_PUBLIC_P2P_IP=203.0.113.12
JOINER_P2P_PORT=9040
JOINER_RPC_PORT=18040

# Seeds the local node should dial first (comma separated)
SEED_PEERS=203.0.113.10:9040,203.0.113.11:9040
